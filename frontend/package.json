{
  "name": "stackpacks-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@segment/analytics-next": "^1.64.0",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.7",
    "axios-case-converter": "^1.1.1",
    "brace": "^0.11.1",
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "deep-equal": "^2.2.3",
    "flowbite-react": "^0.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2",
    "tailwindcss": "^3.4.1",
    "ts-deepmerge": "^7.0.0",
    "tw-merge": "^0.0.1-alpha.3",
    "uuid": "^9.0.1",
    "vite": "^5.1.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "yaml": "^2.3.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.3.22",
    "flowbite-typography": "^1.0.3",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.71.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.3.1"
  },
  "overrides": {
    "react-scripts": {
      "react-refresh": "^0.14.0"
    }
  },
  "scripts": {
    "start": "vite --port 3100",
    "build": "tsc && vite build",
    "serve": "vite preview --port 3100",
    "serve:dev": "npm run build:dev && vite preview --mode production --port 3100",
    "serve:prod": "npm run build:prod && vite preview --mode production --port 3100",
    "build-dev": "env-cmd -f ./.env.dev npm run build",
    "build-prod": "env-cmd -f ./.env.prod npm run build",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "clean": "rm -rf ./dist",
    "fixup": "npx prettier --write . && npx eslint . --fix",
    "reinit": "rm -rf node_modules && rm -rf package-lock.json && npm install"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vitest/recommended",
      "eslint:recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:tailwindcss/recommended",
      "react-app"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "vitest",
      "unused-imports",
      "react-refresh"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "react-refresh/only-export-components": "warn",
      "tailwindcss/classnames-order": [
        "error",
        {
          "callees": [
            "classnames"
          ],
          "config": "./tailwind.config.ts"
        }
      ],
      "tailwindcss/no-custom-classname": [
        "off",
        {
          "config": "./tailwind.config.ts"
        }
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3000",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,ejs,cjs,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6",
    "@swc/core": "^1.4.4",
    "@swc/core-darwin-arm64": "^1.4.4",
    "@swc/core-darwin-x64": "^1.4.4",
    "@swc/core-linux-arm-gnueabihf": "^1.4.4",
    "@swc/core-linux-arm64-gnu": "^1.4.4",
    "@swc/core-linux-arm64-musl": "^1.4.4",
    "@swc/core-linux-x64-gnu": "^1.4.4",
    "@swc/core-linux-x64-musl": "^1.4.4",
    "@swc/core-win32-arm64-msvc": "^1.4.4",
    "@swc/core-win32-ia32-msvc": "^1.4.4",
    "@swc/core-win32-x64-msvc": "^1.4.4"
  }
}
